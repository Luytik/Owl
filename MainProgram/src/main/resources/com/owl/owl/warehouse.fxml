<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="448.0" prefWidth="701.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.owl.owl.WarehouseController">
   <children>
      <Label layoutX="278.0" layoutY="14.0" prefHeight="49.0" prefWidth="93.0" text="Warehouse">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="updateBtn" layoutX="31.0" layoutY="26.0" mnemonicParsing="false" onAction="#updateGoodsList" prefHeight="37.0" prefWidth="116.0" text="Оновити" />
      <TableView fx:id="GoodsList" layoutX="31.0" layoutY="84.0" prefHeight="323.0" prefWidth="647.0">
        <columns>
          <TableColumn fx:id="idClmn" prefWidth="89.0" text="Номер" />
          <TableColumn fx:id="nameClmn" prefWidth="260.0" text="Назва" />
            <TableColumn fx:id="unitClmn" prefWidth="94.0" text="Міра вимірювання" />
            <TableColumn fx:id="amountClmn" prefWidth="94.0" text="Кількість" />
            <TableColumn fx:id="statusClmn" prefWidth="109.0" text="Статус" />
        </columns>
      </TableView>
      <Button fx:id="adddGoodsBtn" layoutX="562.0" layoutY="26.0" mnemonicParsing="false" onAction="#addGoods" prefHeight="37.0" prefWidth="116.0" text="Додати товар" />
   </children>
</AnchorPane>
